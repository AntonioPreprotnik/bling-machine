{
  "name": "dev-example-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rimraf resources/app/public/js && rm -rf .shadow-cljs",
    "postcss": "NODE_ENV=production postcss src/frontend/app/ui/style.css -o resources/public/assets/css/style.css",
    "postcss:watch": "NODE_ENV=development TAILWIND_MODE=watch postcss src/frontend/app/ui/style.css -o resources/public/assets/css/style.css -w  --verbose",
    "develop": "npm run postcss:watch",
    "build": "npm run postcss"
  },
  "dependencies": {
    "@js-joda/core": "^3.2.0",
    "@js-joda/locale_en-us": "^3.2.2",
    "@js-joda/timezone": "2.5.0",
    "create-react-class": "~15.7.0",
    "cssnano": "^5.0.5",
    "jsdom": "^16.0.1",
    "jsdom-global": "^3.0.2",
    "react": "~17.0.1",
    "react-dom": "~17.0.2",
    "react-refresh": "0.8.1",
    "rimraf": "~3.0.0",
    "serve-handler": "~6.1.2",
    "zprint-clj": "~0.8.0"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@tailwindcss/typography": "^0.5.2",
    "autoprefixer": "^10.4.2",
    "postcss": "^8.4.6",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.1",
    "purgecss": "^4.1.0",
    "shadow-cljs": "~2.18.0",
    "tailwindcss": "^3.0.22"
  }
}
